{
  "name": "react-starter-app",
  "version": "1.0.0",
  "description": "This is a setup project to start any react web project with webpack, babel and type script",
  "main": "src/index.tsx",
  "scripts": {
    "start:dev": "webpack serve --mode development --hot --config webpack.config.js",
    "build": "webpack --mode production",
    "start:prod": "npm run build && live-server dist"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0",
    "live-server": "^1.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.3",
    "webpack": "^5.21.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
